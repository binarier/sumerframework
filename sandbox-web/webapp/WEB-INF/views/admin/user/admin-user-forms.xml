<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean name="admin/user#browse#forms" class="com.huateng.sumer.runtime.web.meta.FormDefinition">
		<property name="layouts">
				<bean class="com.huateng.sumer.runtime.web.meta.BrowserLayout" p:caption="浏览">
					<property name="fields">
						<list>
							<bean parent="tbl_adm_user__user_id"/>
						</list>
					</property>
				</bean>
		</property>
	</bean>
</beans>
